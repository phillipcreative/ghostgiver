<div class="banner-relative Desktop">
  <img class="bg-main-works " src="{{section.settings.imgbanner | img_url: 'master'}}">
  <div class="banner-overlay-text">
    <div class="container right-banner">
      <div class="bs-vert " data-aos-delay="10000000">
        <div class="bs-content">{{ section.settings.bannername }}</div>
      </div>
    </div>
  </div>
</div>

<div class="banner-relative mobile" style="display:none;">
  <img class="bg-main-works " src="{{section.settings.imgbanner_mobile | img_url: 'master'}}">
  <div class="banner-overlay-text">
    <div class="container right-banner">
      <div class="bs-vert " data-aos-delay="10000000">
        <div class="bs-content">{{ section.settings.bannername }}</div>
      </div>
    </div>
  </div>
</div>

<div class="about_us" style="background-image: url('{{ section.settings.bg_image |img_url:"master" }} ');">
  <div class="container">
    <div class="row">
         <div class="not-tabs">
     


            <a href="{{ section.settings.link_whoweare }}">{{ section.settings.tab1_name }}</a>
      
        
            <a class="active" href="javascript:void(0)">{{ section.settings.tab2_name }}</a>
     
  
  

        <div id="tabs-content">
      <div id="tab2" class="tab-content">
        <img src="{{ section.settings.img_ghos_foundation | img_url:'master' }}">
        <div class="foundation_img_text">
          <div class="image_left_abt">
            <img src="{{ section.settings.img_ghost_right | img_url:'master' }}">
          </div>
          <div class="text_left_abt">
            <span>{{ section.settings.text_left }}</span>
          </div>
        </div>
        <div class="foundation_point">
          <h2>{{ section.settings.poins_head }}</h2>
          <div class="ponts_cstm1">
            <div class="custm-point-one">
              <h3>{{ section.settings.point1_head }}</h3>
              <p>{{ section.settings.point1_text }}</p>
            </div>
            <div class="btn">
              <a class="btn btn-purple" href="{{ section.settings.btn_poit1_url }}">
                <svg xmlns="http://www.w3.org/2000/svg" width="53.097" height="49.144" viewBox="0 0 53.097 49.144">
